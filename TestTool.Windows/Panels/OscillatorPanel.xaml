<UserControl x:Class="TestTool.Windows.Panels.OscillatorPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestTool.Windows.Panels"
             xmlns:Views="clr-namespace:TestTool.Windows.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Views:Oscillator x:Name="osc1" FrequencyUpdated="osc_FrequencyUpdated"/>
        <Views:Oscillator x:Name="osc2" Grid.Row="1" FrequencyUpdated="osc_FrequencyUpdated"/>
    </Grid>
</UserControl>
