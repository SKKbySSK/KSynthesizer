<UserControl x:Class="GSynthesizer.Windows.Views.FilterModeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True" Width="100" Height="110">
    <StackPanel Background="White">
        <RadioButton x:Name="disabled" IsChecked="True" Content="Disabled" HorizontalAlignment="Left" VerticalAlignment="Top" Checked="disabled_Checked"/>
        <RadioButton x:Name="lpf" Content="LPF" HorizontalAlignment="Left" VerticalAlignment="Top" Checked="lpf_Checked"/>
        <RadioButton x:Name="hpf" Content="HPF" HorizontalAlignment="Left" VerticalAlignment="Top" Checked="hpf_Checked"/>
        <RadioButton x:Name="bpf" Content="BPF" HorizontalAlignment="Left" VerticalAlignment="Top" Checked="bpf_Checked"/>
    </StackPanel>
</UserControl>
